{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "graphql",
    "html",
    "yaml",
    "json",
    "python",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "tsserver.formatOnType": true,
